{
    "useTabs": false,
    "singleQuote": false,
    "trailingComma": "all",
    "bracketSameLine": false,
    "endOfLine": "lf",
    "semi": true,
    "tabWidth": 4,

    "printWidth": 100,
    "plugins": ["prettier-plugin-svelte"],
    "pluginSearchDirs": ["."],
    "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
